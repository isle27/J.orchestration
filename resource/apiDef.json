{
    "manager": {
        "assign": [
            {
                "type": "incode",
                "action": "filter",
                "data": "incidents",
                "operation": "ticket_status==Backlog"
            },
            {
                "type": "incode",
                "action": "selectOne",
                "data": "local_selection",
                "operation": null
            },
            {
                "type": "incode",
                "action": "selectOne",
                "data": "users",
                "operation": null
            },
            {
                "type": "api",
                "method": "PUT",
                "route":"/incidents",
               
                "route_extension":"local_selection.incident_id",
                "body":{
                    "engineerId":"collection.users.id",
                    "deadline":"date+1",
                    "remark":"random-sentence"
                },
                "operation": "status==backlog"
            }
        ],
        "reassign":[
            {
                "type": "incode",
                "action": "filter",
                "data": "incidents",
                "operation": "ticket_status==InProgress"
            },
             {
                "type": "incode",
                "action": "selectOne",
                "data": "local_selection",
                "operation": null
            },
             {
                "type": "incode",
                "action": "selectOne",
                "data": "users",
                "operation": null
            },
            {
                "type": "api",
                "method": "PUT",
                "route":"/reAssign",
               
                "route_extension":"local_selection.incident_id",
                "body":{
                    "engineerId":"collection.users.id",
                    "deadline":"date+1",
                    "remark":"random-sentence"
                },
                "operation": "status==backlog"
            }

        ]
        
    }
}